    "class-validator": "^0.14.0",
    "http-errors": "^2.0.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "jwk-to-pem": "^2.0.7",
    "jwt-decode": "^4.0.0",
    "nestjs-pino": "^4.4.0",
    "nodemailer": "^7.0.3",
    "pg": "^8.16.0",
    "pino-http": "^10.5.0",
    "pino-mongodb": "^4.3.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.0.0",
