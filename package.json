  "scripts": {
    "build": "nest build",
    "build:user-service": "nest build user-service",
    "build:api-gateway": "nest build api-gateway",
    "start:user-service": "nest start user-service",
    "start:api-gateway": "nest start api-gateway",
    "dev:user-service": "nest start user-service --watch",
    "dev:api-gateway": "nest start api-gateway --watch",
    "dev": "concurrently \"npm run dev:user-service\" \"npm run dev:api-gateway\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.826.0",
    "@grpc/grpc-js": "^1.10.0",
